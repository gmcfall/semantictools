@prefix lti: <http://www.imsglobal.org/imspurl/lti/v2/vocab/lti#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix lti-type: <http://www.imsglobal.org/imspurl/lti/v2/xsd/ltid#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix bind: <http://purl.org/semantictools/v1/vocab/bind#> .

<http://www.imsglobal.org/imspurl/lti/v2/vocab/lti#> a owl:Ontology  ;
  rdfs:label "LTI Core" ;
  a bind:TargetNamespace ;
  bind:suggestedPrefix "lti" .

lti:BaseUrlChoice rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:default_base_url;
    owl:minCardinality  1;
    rdfs:comment "This is the base URL that should be used by default for all entities specified by the associated BaseUrlSelector.  The secure_base_url attribute may override this default Base URL."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:secure_base_url;
    rdfs:comment "This is the base URL that should be used whenever security is enabled.  If this base URL is not defined, then the default_base_url is used in all circumstances."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:selector;
    rdfs:comment "A resource that specifies the objects to which this BaseUrlChoice applies. "
  ] ; 
  rdfs:comment "Defines the base URL that is used for certain kinds of resources referenced in a ToolProfile.  The kinds of resources to which the base URL applies is specified by an associated BaseUrlSelector.  If the BaseUrlChoice does not have an associated selector, then it applies universally to all resources referenced in the Tool Profile. " .

lti:BaseUrlSelector rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:applies_to;
    owl:minCardinality  1;
    rdfs:comment "The class of resources selected by this BaseUrlSelector."
  ] ; 
  rdfs:comment "This resource specifies the type of objects to which a set of base URLs apply. " .

lti:Contact rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:email;
    owl:minCardinality  1;
    rdfs:comment "The email of the primary contact for the associated object."
  ] ; 
  rdfs:comment "Contact information for the associated object." .

lti:Endpoint rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:path;
    owl:minCardinality  1;
    rdfs:comment """This attribute specifies the path for an endpoint such as a MessageHandler or an Icon. Typically, this path is relative to one of the base URL values defined in a BaseUrlChoice.  
In the case of a MessageHandler, the path may be an absolute path if the associated message type explicitly declares that an absolute path is allowed.  """
  ] ; 
  rdfs:comment "This is the abstract base class for objects that have an endpoint such as icons or message handlers. " .

lti:ServiceProvider rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:guid;
    owl:minCardinality  1;
    rdfs:comment "A globally unique identifier for the service provider.  As a best practice, this value should match an Internet domain name assigned by ICANN, but any globally unique identifier is acceptable.  "
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:timestamp;
    owl:minCardinality  1;
    rdfs:comment "A timestamp for the ServiceProvider record. This value is useful for determining which record is most current."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:provider_name;
    owl:minCardinality  1;
    rdfs:comment "The name of the service provider, suitable for display to end users."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:description;
    rdfs:comment "A description of the service provider, suitable for display to end-users."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:support;
    rdfs:comment "Contact information for support from a service provider."
  ] ; 
  rdfs:comment "This resource represents the service provider that hosts a product (Tool or Tool Consumer)." .

lti:IconEndpoint rdf:type owl:Class ;
  rdfs:subClassOf lti:Endpoint ; 
  rdfs:comment "Defines the path for an Icon.  " .

lti:IconInfo rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:key;
    rdfs:comment "If localization is supported then the 'key' attribute identifies a property in the resource bundle provided in response to the get-bundle-request message. The value of that property is the locale-specific path to the icon."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:default_location;
    rdfs:comment "Specifies the default path to an icon.  The absolute URL is formed by appending this path to the appropriate base URL for icons."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:icon_style;
    rdfs:comment "The style of the icon referenced by this IconInfo."
  ] ; 
  rdfs:comment "Contains information about a graphical icon that may be displayed in the Tool Consumer's user interface." .

lti:IconStyle rdf:type owl:Class ; 
  rdfs:comment """<p>A resource that specifies the style for an Icon.</p>
The LTI standard does not define any particular Icon styles.  Each vendor is free to publish its own set of icon styles.  A typical style might specify dimensions of the icon or its intended usage.  Each IconStyle is uniquely identified by a URI.  Ideally, the URI should resolve to a resource that provides a definition of the icon style.""" .

lti:LocalizedName rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:key;
    rdfs:comment "The key used to lookup the locale-specific value from a resource bundle."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:default_value;
    owl:onClass         lti-type:LongName.Type;
    rdfs:comment "The default value for the display name.  This value is used if (1) the key attribute is undefined, (2) the localization capability is not enabled, or (3) a value for the specified key is not found in the locale-specific resource bundle."
  ] ; 
  rdfs:comment "This container stores the default display name for some object plus a key that may be used to lookup a translation for the display name from a resource bundle for a particular locale.  The string value has a maximum length of 128 characters." .

lti:LocalizedText rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:key;
    rdfs:comment "If localization is supported then the key identifies a property in the bundle returned by the get-bundle-request message handler. "
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:default_value;
    owl:onClass         lti-type:Text.Type;
    rdfs:comment "This is the default value for the text. This default value  is used if (1) the key is not specified, (2) the localization capability is not enabled, or (3) the specified key is not found in the locale-specific resource bundle."
  ] ; 
  rdfs:comment "This container defines a block of text.  The container includes a default value for the text, plus a key that can be used to lookup a locale-specific value from a resource bundle." .

lti:MessageHandler rdf:type owl:Class ;
  rdfs:subClassOf lti:Endpoint ,
  [ a owl:Restriction ;
    owl:onProperty      lti:enabled_capability;
    rdfs:comment "A capability that is enabled for messages handled by this MessageHandler."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:message_type;
    owl:minCardinality  1;
    rdfs:comment "The type of message that can be submitted to the Message Handler."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:parameter;
    rdfs:comment "Specifies a custom parameter that is expected by the MessageHandler."
  ] ; 
  rdfs:comment "Defines the features of a message handler, including information about the handler's service endpoint and custom parameters that it expects." .

lti:ProductInfo rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:product_name;
    owl:minCardinality  1;
    rdfs:comment "A name for the product, suitable for display to end users."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:product_version;
    owl:minCardinality  1;
    rdfs:comment "This is the version of the product. "
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:description;
    rdfs:comment "This is a description of the product suitable for display to end-users."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:technical_description;
    rdfs:comment "This is a human readable description of the technical aspects of this product that might be of interest to developers who wish to integrate with this product via LTI."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:product_family;
    owl:minCardinality  1;
    rdfs:comment "An inverse attribute that references the ProductFamily within which this ProductInfo is defined."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:instance;
    rdfs:comment "A deployed instance of a particular version of a product (Tool or Tool Consumer)."
  ] ; 
  rdfs:comment "Encapsulates information about one released version of a product (Tool Consumer or Tool)." .

lti:ProductFamily rdf:type owl:Class ;
  rdfs:subClassOf lti:HasCode ,
  [ a owl:Restriction ;
    owl:onProperty      lti:vendor;
    owl:minCardinality  1;
    rdfs:comment "The vendor that owns the ProductFamily."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:version;
    rdfs:comment "Encapsulates information about one version of the product."
  ] ; 
  rdfs:comment """A ProductFamily represents the collection of all versions of a particular product over time.  Two products that have the same product <em>code</em> belong to the same ProductFamily.  The LTI product model has four levels: <OL>
  <LI> <code>ProductFamily</code>:  The collection of all versions of a given product.
  <LI> <code>ProductInfo</code>: Encapsulates information about one particular version of a product.
  <LI> <code>ProductInstance</code>: Represents one deployed instance of a particular version of a product.
  <LI> <code>ProductProfile</code>: Defines a <em>view</em> of a deployed instance of a product.  This view may be customized for different integration partners.
</OL>

Notice that the ProductFamily sits at the top of the product hierarchy.  Each product family is owned by a <code>Vendor</code>.""" .

lti:ProductProfile rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:lti_version;
    owl:onClass         xsd:normalizedString;
    owl:minQualifiedCardinality  1;
    rdfs:comment "The identifier for an LTI version that the version supports.  A given product (Tool or Tool Consumer) may support multiple versions, but only one version is selected for use in the integration contract."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:service_offered;
    rdfs:comment "The descriptor for a service offered by the product (Tool or Tool Consumer)."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:product_instance;
    owl:minCardinality  1;
    rdfs:comment "An inverse attribute which references the ProductInstance within which this ProductProfile is defined."
  ] ; 
  rdfs:comment """A ProductProfile presents an integration view of a deployed instance of some product (Tool or Tool Consumer).  It defines the services and other features offered to an integration partner.  A profile may be customized for each integration partner or shared with multiple partners.
<p>ProductProfile is the abstract base class for all product profiles. The ToolConsumerProfile and ToolProfile are derived from this base class.  </p>""" .

lti:ServiceDescriptor rdf:type owl:Class ; 
  rdfs:comment """This entity defines a profile for a service.  It is a \"profile\" in the sense that it specifies a subset of operations that are declared by the full  interface for some service.  For example, a profile might list the \"read\" operations of a service but exclude the \"write\" operations.  This is useful because a product (Tool or TC) might want to offer the read-only operations to a partner, but it might not want to expose write operations.  (On the other hand, it is perfectly legitimate for a profile to list all of the operations that are declared in the service interface if desired.)
ServiceProfiles are used for four different purposes: (1) they allow a product to advertise the operations that it offers to a partner, (2) they allow a product to declare the operations that it intends to use, (3) they allow a product to advertise the security profiles that are available for service operations, and (4) they allow a product to assert the security profiles that will be used for service operations. """ .

lti:ToolConsumerProfile rdf:type owl:Class ;
  rdfs:subClassOf lti:ProductProfile ,
  [ a owl:Restriction ;
    owl:onProperty      lti:guid;
    owl:minCardinality  1
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:capability_offered;
    rdfs:comment "A capability offered by the Tool Consumer to its integration partners."
  ] ; 
  rdfs:comment "The profile for a Tool Consumer product." .

lti:ToolProfile rdf:type owl:Class ;
  rdfs:subClassOf lti:ProductProfile ,
  [ a owl:Restriction ;
    owl:onProperty      lti:base_url_choice;
    owl:minCardinality  1
  ] ; 
  rdfs:comment "The profile for a Tool product." .

lti:ResourceHandler rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:name;
    owl:onClass         lti:LocalizedName;
    owl:minQualifiedCardinality  1;
    rdfs:comment "The name of the resource handler suitable for display in the TC user interface."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:resource_type;
    owl:minCardinality  1;
    rdfs:comment "Identifies the type of resource associated with this ResourceHandler.  Resource types are identified by a code that is unique within a given ProductFamily."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:message;
    owl:minCardinality  1;
    rdfs:comment "Defines the features of a message handler associated with the resource described by this ResourceHandler."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:icon_info;
    rdfs:comment "Provides information about a graphical icon that represents the resource described by this ResourceHandler."
  ] ; 
  rdfs:comment "The set of information for a specific resource handler." .

lti:Parameter rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:name;
    owl:onClass         xsd:Name;
    owl:minQualifiedCardinality  1;
    rdfs:comment "The name of the parameter."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:fixed;
    rdfs:comment "Denotes that the value of the parameter is fixed at the time of definition within the product profile.  This value must be passed in each message of the associated message type."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:variable;
    rdfs:comment "Denotes that the parameter has a variable value that is automatically substituted at run-time.  The value of this attribute is the name for a variable whose value will be added to the message at run-time. "
  ] ; 
  rdfs:comment "Defines the characteristics of a parameter expected by a MessageHandler." .

lti:ShortLocalizedName rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:key;
    rdfs:comment "The key used to lookup the locale-specific value from a resource bundle."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:default_value;
    owl:onClass         lti-type:ShortName.Type;
    rdfs:comment "The default value for the display name.  This value is used if (1) the key attribute is undefined, (2) the localization capability is not enabled, or (3) a value for the specified key is not found in the locale-specific resource bundle."
  ] ; 
  rdfs:comment "This container stores the default display name for some object plus a key that may be used to lookup a translation for the display name from a resource bundle for a particular locale.  This class is similar to LocalizedName, except that that the string value is limited to 64 characters." .

lti:Vendor rdf:type owl:Class ;
  rdfs:subClassOf lti:HasCode ,
  [ a owl:Restriction ;
    owl:onProperty      lti:timestamp;
    owl:minCardinality  1;
    rdfs:comment "A timestamp for the Vendor record. This value is useful for determining which record is most current."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:name;
    owl:onClass         lti:LocalizedName;
    owl:minQualifiedCardinality  1;
    rdfs:comment "Defines a human readable name for the Vendor.  The name should be suitable for display in management screens within the Tool Provider system."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:description;
    rdfs:comment "This is a human-readable description of the Vendor."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:website;
    rdfs:comment "This is the URL of the vendor."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:product;
    rdfs:comment """<p>A group of related products offered by the Vendor.  All of the products within a given product family are different <em>versions</em> of the same product. </p>
<p>A vendor may have multiple product families.  For example, the same vendor might offer a discussion forum tool and also a white board tool.  Each of these tools corresponds to a different product family.</p>"""
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:contact;
    rdfs:comment "Contact information for this Vendor."
  ] ; 
  rdfs:comment "A person or agency that offers products to the market. The key types of products relevant to LTI are Tools (which are described by Tool Profiles) and Tool Consumers (which are described by Tool Consumer Profiles)." .

lti:Capability rdf:type owl:Class ; 
  rdfs:comment "Represents a capability that may be declared by a Tool Consumer in its profile and enabled by a Tool when an integration contract is established." .

lti:MessageType rdf:type owl:Class ;
  rdfs:subClassOf lti:Capability ,
  [ a owl:Restriction ;
    owl:onProperty      lti:absolute_path_allowed;
    rdfs:comment "Specifies whether the path for this message type is allowed to be specified using an absolute path.  A value of \"false\" means that the MessageHandler must declare a relative path."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:message_class;
    owl:minCardinality  1;
    rdfs:comment "This attribute identifies the formal Class definition the type of message that can be delivered to a given MessageHandler."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:http_method;
    owl:minCardinality  1;
    rdfs:comment "Specifies an HTTP method (POST, GET, PUT, etc.) for the type of messages represented by this MessageType."
  ] ; 
  rdfs:comment """This container holds metadata about a particular type of message.
It provides a friendly name (an alias) for the formal Message class name, and it specifies the allowed HTTP methods for Messages of the given type.""" .

lti:SecurityContract rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:shared_secret;
    owl:minCardinality  1;
    rdfs:comment """This is the shared secret used for communications between the Tool Consumer and the Tool Provider.  It is expected that the Tool Consumer present an interface to the administrative user for them to activate the Tool Proxy before this secret is ever used by the Tool Consumer. 
The shared secret may be updated when a tool is re-registered (see the reregisterTool operation of the ToolRegistrationService).  In addition, the TC and TP should provide interfaces which allow their respective administrators to change the shared secret through an out-of-band agreement."""
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:tool_service;
    rdfs:comment "The profile for a Tool Consumer service that may be invoked by the Tool acting on its own (as opposed to acting as an agent for some end user).  "
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:end_user_service;
    rdfs:comment "The profile for a Tool Consumer service that may be invoked by the Tool when acting as an agent for some end user.  "
  ] ; 
  rdfs:comment """This entity specifies all aspects of the security agreements between Tool Provider and Tool Consumer.
<p>
Notice that the security contract distinguishes between <code>tool_service</code>s and <code>end_user_service</code>s.  A <code>tool_service</code> is a service that the Tool invokes when acting on its own.  By contrast, a <code>end_user_service</code> is a service that the Tool invokes when acting as an agent for some end user.  Consider, for example, the case where a Tool is reporting scores to the Tool Consumer via an outcomes service.  When a student submits an assignment, the Tool may calculate a score and post it to the Tool Consumer.  In this case the Tool is acting on its own since students generally do not have permission to enter their own scores into a grade book.  Now, suppose the Tool exposes a user interface which allows instructors to change a score.  In this case, when the Tool posts the modified score to the Tool Consumer grade book, it is acting as an agent for the end user.  This example, illustrates that the same service may be referenced as both a <code>tool_service</code> and a <code>end_user_service</code>
</p>
""" .

lti:ServiceProfile rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:service;
    owl:minCardinality  1;
    rdfs:comment "The descriptor for the service that is being profiled."
  ] ; 
  rdfs:comment "A profile for some service that may be invoked by an integration partner.  A profile specifies which methods or operations the integration partner intends to use." .

lti:RestServiceProfile rdf:type owl:Class ;
  rdfs:subClassOf lti:ServiceProfile ,
  [ a owl:Restriction ;
    owl:onProperty      lti:action;
    owl:minCardinality  1;
    rdfs:comment "Specifies an HTTP method (POST, GET, PUT, etc.) which the integration partner intends to use.   A RestServiceProfile may include more than one action.  However, the set of actions declared in a RestServiceProfile must be a subset of actions declared in the RestService referenced by the <code>service</code> property."
  ] ; 
  rdfs:comment "A kind of ServiceProfile specialized for REST services. " .

lti:ResourceType rdf:type owl:Class ;
  rdfs:subClassOf lti:HasCode ,
  [ a owl:Restriction ;
    owl:onProperty      lti:resource_type_owner;
    owl:minCardinality  1
  ] ; 
  rdfs:comment "This class represents all ResourceHandlers of a given type.  Two ResourceHandlers that belong to this set are considered to be different versions of the same ResourceHandler." .

lti:HasCode rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:code;
    owl:minCardinality  1;
    rdfs:comment "A unique identifier for the resource."
  ] ; 
  rdfs:comment "A resource that can be identified by a <code>code</code> property.  The code property may be globally unique (as in the case of a Vendor resource) or locally unqiue (as in the case of a Product Family, where the code is locally unique within the scope of the parent Vendor resource)." .

lti:RestService rdf:type owl:Class ;
  rdfs:subClassOf lti:ServiceDescriptor ,
  [ a owl:Restriction ;
    owl:onProperty      lti:format;
    owl:minCardinality  1;
    rdfs:comment "The content type (also known as media type) of resources accessed via the endpoint defined by this RestService descriptor.   The endpoint of a REST service may support more than one content type if content negotiation is enabled as described in the <a href=\"http://www.w3.org/Protocols/rfc2616/rfc2616-sec12.html\">HTTP 1.1</a> specification."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:endpoint;
    owl:minCardinality  1;
    rdfs:comment "A URI template that defines the REST endpoint for resources that may be accessed via the REST service.  The URI template should conform to the syntax in the proposed IETF standard [<a href=\"http://www.ietf.org/id/draft-gregorio-uritemplate-08.txt\">draft-gregorio-uritemplate-08</a>]. "
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:action;
    owl:minCardinality  1;
    rdfs:comment "Specifies an HTTP action that is supported at the REST endpoint.  A given endpoint may support more than one action.  For example, it is common practice for a collection endpoint to accept POST requests, and a different item endpoint to accept GET, PUT, and DELETE requests."
  ] ; 
  rdfs:comment "A descriptor for a REST service." .

lti:ProductInstance rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:guid;
    owl:minCardinality  1
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:service_provider;
    rdfs:comment "This is the service provider that hosts the deployed product."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:product_info;
    owl:minCardinality  1;
    rdfs:comment "This is metadata about the product described in the profile."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:profile;
    rdfs:comment "A resource that presents a view of this ProductInstance to one or more integration partners."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:support;
    rdfs:comment "Contact information for support on this deployed instance of the product (Tool or ToolConsumer)."
  ] ; 
  rdfs:comment "The resource encapsulates information about one deployed instance of a web-based product (Tool or Tool Consumer)." .

lti:ToolProxy rdf:type owl:Class;
  rdfs:subClassOf 
  [ a owl:Restriction ;
    owl:onProperty      lti:lti_version;
    owl:onClass         xsd:token;
    owl:minQualifiedCardinality  1;
    rdfs:comment "The LTI version that governs the integration between the Tool and ToolConsumer. "
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:tool_proxy_guid;
    owl:minCardinality  1;
    rdfs:comment "The globally unique identifier for the ToolProxy (a.k.a. integration contract)."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:tool_profile;
    owl:minCardinality  1;
    rdfs:comment "The profile of the Tool that is participating in the integration governed by this ToolProxy."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:security_contract;
    owl:minCardinality  1;
    rdfs:comment "The security aspect of an integration contract between two parties."
  ] ,
  [ a owl:Restriction ;
    owl:onProperty      lti:tool_consumer_profile;
    owl:minCardinality  1;
    rdfs:comment "The profile of the Tool Consumer that is participating in the integration governed by this ToolProxy."
  ] ; 
  rdfs:comment "This entity defines the integration contract between a ToolConsumer and a Tool.   It is called a \"ToolProxy\" because its primary purpose is to provide the ToolConsumer with all of the information needed to embed a given Tool within the ToolConsumer system." .

lti:HttpMethod a owl:Class ;
  owl:equivalentClass [
    owl:oneOf (
      lti:POST 
      lti:GET 
      lti:PUT 
      lti:DELETE
    )
  ] ;
  rdfs:comment "Enumerates the HTTP methods that can be used when sending a message, as defined by IETF RFC 2616 ." .

lti:default_base_url rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:BaseUrlChoice ;
  rdfs:range xsd:anyURI ;
  rdfs:comment "This is the base URL that should be used by default for all entities specified by the associated BaseUrlSelector.  The secure_base_url attribute may override this default Base URL." .

lti:secure_base_url rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:BaseUrlChoice ;
  rdfs:range xsd:anyURI ;
  rdfs:comment "This is the base URL that should be used whenever security is enabled.  If this base URL is not defined, then the default_base_url is used in all circumstances." .

lti:selector rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:BaseUrlChoice ;
  rdfs:range lti:BaseUrlSelector ;
  rdfs:comment "A resource that specifies the objects to which this BaseUrlChoice applies. " .

lti:applies_to rdf:type rdf:Property ;
  rdfs:domain lti:BaseUrlSelector ;
  rdfs:range owl:Class ;
  rdfs:comment "The class of resources selected by this BaseUrlSelector." .

lti:email rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:Contact ;
  rdfs:range xsd:normalizedString ;
  rdfs:comment "The email of the primary contact for the associated object." .

lti:path rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:Endpoint ;
  rdfs:range xsd:anyURI ;
  rdfs:comment """This attribute specifies the path for an endpoint such as a MessageHandler or an Icon. Typically, this path is relative to one of the base URL values defined in a BaseUrlChoice.  
In the case of a MessageHandler, the path may be an absolute path if the associated message type explicitly declares that an absolute path is allowed.  """ .

lti:guid rdf:type owl:FunctionalProperty ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:ServiceProvider
      lti:ToolConsumerProfile
      lti:ProductInstance
    )
  ] ;
  rdfs:range lti-type:GUID.Type ;
  rdfs:comment "A globally unique identifier for the service provider.  As a best practice, this value should match an Internet domain name assigned by ICANN, but any globally unique identifier is acceptable.  " .

lti:timestamp rdf:type owl:FunctionalProperty ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:ServiceProvider
      lti:Vendor
    )
  ] ;
  rdfs:range xsd:dateTime ;
  rdfs:comment "A timestamp for the ServiceProvider record. This value is useful for determining which record is most current." .

lti:provider_name rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ServiceProvider ;
  rdfs:range lti:LocalizedName ;
  rdfs:comment "The name of the service provider, suitable for display to end users." .

lti:description rdf:type owl:FunctionalProperty ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:ServiceProvider
      lti:ProductInfo
      lti:ResourceHandler
      lti:Vendor
    )
  ] ;
  rdfs:range lti:LocalizedText ;
  rdfs:comment "A description of the service provider, suitable for display to end-users." .

lti:support rdf:type owl:FunctionalProperty ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:ServiceProvider
      lti:ProductInstance
    )
  ] ;
  rdfs:range lti:Contact ;
  rdfs:comment "Contact information for support from a service provider." .

lti:key rdf:type owl:FunctionalProperty ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:IconInfo
      lti:LocalizedName
      lti:LocalizedText
      lti:ShortLocalizedName
    )
  ] ;
  rdfs:range lti-type:Name.Type ;
  rdfs:comment "If localization is supported then the 'key' attribute identifies a property in the resource bundle provided in response to the get-bundle-request message. The value of that property is the locale-specific path to the icon." .

lti:default_location rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:IconInfo ;
  rdfs:range lti:IconEndpoint ;
  rdfs:comment "Specifies the default path to an icon.  The absolute URL is formed by appending this path to the appropriate base URL for icons." .

lti:icon_style rdf:type rdf:Property ;
  rdfs:domain lti:IconInfo ;
  rdfs:range lti:IconStyle ;
  rdfs:comment "The style of the icon referenced by this IconInfo." .

lti:default_value rdf:type owl:FunctionalProperty ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:LocalizedName
      lti:LocalizedText
      lti:ShortLocalizedName
    )
  ] ;
  rdfs:range  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti-type:LongName.Type
      lti-type:Text.Type
      lti-type:ShortName.Type
    )
  ] ;
  rdfs:comment "The default value for the display name.  This value is used if (1) the key attribute is undefined, (2) the localization capability is not enabled, or (3) a value for the specified key is not found in the locale-specific resource bundle." .

lti:enabled_capability rdf:type rdf:Property ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:MessageHandler
      lti:ToolProxy
    )
  ] ;
  rdfs:range lti:Capability ;
  rdfs:comment "A capability that is enabled for messages handled by this MessageHandler." .

lti:message_type rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:MessageHandler ;
  rdfs:range lti:MessageType ;
  rdfs:comment "The type of message that can be submitted to the Message Handler." .

lti:parameter rdf:type rdf:Property ;
  rdfs:domain lti:MessageHandler ;
  rdfs:range lti:Parameter ;
  rdfs:comment "Specifies a custom parameter that is expected by the MessageHandler." .

lti:product_name rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ProductInfo ;
  rdfs:range lti:ShortLocalizedName ;
  rdfs:comment "A name for the product, suitable for display to end users." .

lti:product_version rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ProductInfo ;
  rdfs:range xsd:normalizedString ;
  rdfs:comment "This is the version of the product. " .

lti:technical_description rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ProductInfo ;
  rdfs:range lti:LocalizedText ;
  rdfs:comment "This is a human readable description of the technical aspects of this product that might be of interest to developers who wish to integrate with this product via LTI." .

lti:product_family rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ProductInfo ;
  rdfs:range lti:ProductFamily ;
  owl:inverseOf lti:version ;
  rdfs:comment "An inverse attribute that references the ProductFamily within which this ProductInfo is defined." .

lti:instance rdf:type rdf:Property ;
  rdfs:domain lti:ProductInfo ;
  rdfs:range lti:ProductInstance ;
  owl:inverseOf lti:product_info ;
  rdfs:comment "A deployed instance of a particular version of a product (Tool or Tool Consumer)." .

lti:vendor rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ProductFamily ;
  rdfs:range lti:Vendor ;
  owl:inverseOf lti:product ;
  rdfs:comment "The vendor that owns the ProductFamily." .

lti:version rdf:type rdf:Property ;
  rdfs:domain lti:ProductFamily ;
  rdfs:range lti:ProductInfo ;
  owl:inverseOf lti:product_family ;
  rdfs:comment "Encapsulates information about one version of the product." .

lti:lti_version rdf:type owl:FunctionalProperty ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:ProductProfile
      lti:ToolProxy
    )
  ] ;
  rdfs:range  [
    rdf:type owl:Class ;
    owl:unionOf (
      xsd:normalizedString
      xsd:token
    )
  ] ;
  rdfs:comment "The identifier for an LTI version that the version supports.  A given product (Tool or Tool Consumer) may support multiple versions, but only one version is selected for use in the integration contract." .

lti:service_offered rdf:type rdf:Property ;
  rdfs:domain lti:ProductProfile ;
  rdfs:range lti:ServiceDescriptor ;
  rdfs:comment "The descriptor for a service offered by the product (Tool or Tool Consumer)." .

lti:product_instance rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ProductProfile ;
  rdfs:range lti:ProductInstance ;
  owl:inverseOf lti:profile ;
  rdfs:comment "An inverse attribute which references the ProductInstance within which this ProductProfile is defined." .

lti:capability_offered rdf:type rdf:Property ;
  rdfs:domain lti:ToolConsumerProfile ;
  rdfs:range lti:Capability ;
  rdfs:comment "A capability offered by the Tool Consumer to its integration partners." .

lti:message rdf:type rdf:Property ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:ToolProfile
      lti:ResourceHandler
    )
  ] ;
  rdfs:range lti:MessageHandler .

lti:resource_handler rdf:type rdf:Property ;
  rdfs:domain lti:ToolProfile ;
  rdfs:range lti:ResourceHandler .

lti:base_url_choice rdf:type rdf:Property ;
  rdfs:domain lti:ToolProfile ;
  rdfs:range lti:BaseUrlChoice .

lti:name rdf:type owl:FunctionalProperty ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:ResourceHandler
      lti:Parameter
      lti:Vendor
    )
  ] ;
  rdfs:range  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:LocalizedName
      xsd:Name
    )
  ] ;
  rdfs:comment "The name of the resource handler suitable for display in the TC user interface." .

lti:resource_type rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ResourceHandler ;
  rdfs:range lti:ResourceType ;
  rdfs:comment "Identifies the type of resource associated with this ResourceHandler.  Resource types are identified by a code that is unique within a given ProductFamily." .

lti:icon_info rdf:type rdf:Property ;
  rdfs:domain lti:ResourceHandler ;
  rdfs:range lti:IconInfo ;
  rdfs:comment "Provides information about a graphical icon that represents the resource described by this ResourceHandler." .

lti:fixed rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:Parameter ;
  rdfs:range lti-type:DataValue.Type ;
  rdfs:comment "Denotes that the value of the parameter is fixed at the time of definition within the product profile.  This value must be passed in each message of the associated message type." .

lti:variable rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:Parameter ;
  rdfs:range lti-type:VariableName.Type ;
  rdfs:comment "Denotes that the parameter has a variable value that is automatically substituted at run-time.  The value of this attribute is the name for a variable whose value will be added to the message at run-time. " .

lti:website rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:Vendor ;
  rdfs:range xsd:anyURI ;
  rdfs:comment "This is the URL of the vendor." .

lti:product rdf:type rdf:Property ;
  rdfs:domain lti:Vendor ;
  rdfs:range lti:ProductFamily ;
  owl:inverseOf lti:vendor ;
  rdfs:comment """<p>A group of related products offered by the Vendor.  All of the products within a given product family are different <em>versions</em> of the same product. </p>
<p>A vendor may have multiple product families.  For example, the same vendor might offer a discussion forum tool and also a white board tool.  Each of these tools corresponds to a different product family.</p>""" .

lti:contact rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:Vendor ;
  rdfs:range lti:Contact ;
  rdfs:comment "Contact information for this Vendor." .

lti:absolute_path_allowed rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:MessageType ;
  rdfs:range xsd:boolean ;
  rdfs:comment "Specifies whether the path for this message type is allowed to be specified using an absolute path.  A value of \"false\" means that the MessageHandler must declare a relative path." .

lti:message_class rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:MessageType ;
  rdfs:range owl:Class ;
  rdfs:comment "This attribute identifies the formal Class definition the type of message that can be delivered to a given MessageHandler." .

lti:http_method rdf:type rdf:Property ;
  rdfs:domain lti:MessageType ;
  rdfs:range lti:HttpMethod ;
  rdfs:comment "Specifies an HTTP method (POST, GET, PUT, etc.) for the type of messages represented by this MessageType." .

lti:shared_secret rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:SecurityContract ;
  rdfs:range xsd:string ;
  rdfs:comment """This is the shared secret used for communications between the Tool Consumer and the Tool Provider.  It is expected that the Tool Consumer present an interface to the administrative user for them to activate the Tool Proxy before this secret is ever used by the Tool Consumer. 
The shared secret may be updated when a tool is re-registered (see the reregisterTool operation of the ToolRegistrationService).  In addition, the TC and TP should provide interfaces which allow their respective administrators to change the shared secret through an out-of-band agreement.""" .

lti:tool_service rdf:type rdf:Property ;
  rdfs:domain lti:SecurityContract ;
  rdfs:range lti:ServiceProfile ;
  rdfs:comment "The profile for a Tool Consumer service that may be invoked by the Tool acting on its own (as opposed to acting as an agent for some end user).  " .

lti:end_user_service rdf:type rdf:Property ;
  rdfs:domain lti:SecurityContract ;
  rdfs:range lti:ServiceProfile ;
  rdfs:comment "The profile for a Tool Consumer service that may be invoked by the Tool when acting as an agent for some end user.  " .

lti:service rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ServiceProfile ;
  rdfs:range lti:ServiceDescriptor ;
  rdfs:comment "The descriptor for the service that is being profiled." .

lti:action rdf:type rdf:Property ;
  rdfs:domain  [
    rdf:type owl:Class ;
    owl:unionOf (
      lti:RestServiceProfile
      lti:RestService
    )
  ] ;
  rdfs:range lti:HttpMethod ;
  rdfs:comment "Specifies an HTTP method (POST, GET, PUT, etc.) which the integration partner intends to use.   A RestServiceProfile may include more than one action.  However, the set of actions declared in a RestServiceProfile must be a subset of actions declared in the RestService referenced by the <code>service</code> property." .

lti:resource_type_owner rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ResourceType ;
  rdfs:range lti:ProductFamily .

lti:code rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:HasCode ;
  rdfs:range lti-type:Token.Type ;
  rdfs:comment "A unique identifier for the resource." .

lti:format rdf:type rdf:Property ;
  rdfs:domain lti:RestService ;
  rdfs:range xsd:normalizedString ;
  rdfs:comment "The content type (also known as media type) of resources accessed via the endpoint defined by this RestService descriptor.   The endpoint of a REST service may support more than one content type if content negotiation is enabled as described in the <a href=\"http://www.w3.org/Protocols/rfc2616/rfc2616-sec12.html\">HTTP 1.1</a> specification." .

lti:endpoint rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:RestService ;
  rdfs:range xsd:anyURI ;
  rdfs:comment "A URI template that defines the REST endpoint for resources that may be accessed via the REST service.  The URI template should conform to the syntax in the proposed IETF standard [<a href=\"http://www.ietf.org/id/draft-gregorio-uritemplate-08.txt\">draft-gregorio-uritemplate-08</a>]. " .

lti:service_provider rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ProductInstance ;
  rdfs:range lti:ServiceProvider ;
  rdfs:comment "This is the service provider that hosts the deployed product." .

lti:product_info rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ProductInstance ;
  rdfs:range lti:ProductInfo ;
  owl:inverseOf lti:instance ;
  rdfs:comment "This is metadata about the product described in the profile." .

lti:profile rdf:type rdf:Property ;
  rdfs:domain lti:ProductInstance ;
  rdfs:range lti:ProductProfile ;
  owl:inverseOf lti:product_instance ;
  rdfs:comment "A resource that presents a view of this ProductInstance to one or more integration partners." .

lti:tool_proxy_guid rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ToolProxy ;
  rdfs:range lti-type:GUID.Type ;
  rdfs:comment "The globally unique identifier for the ToolProxy (a.k.a. integration contract)." .

lti:tool_profile rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ToolProxy ;
  rdfs:range lti:ToolProfile ;
  rdfs:comment "The profile of the Tool that is participating in the integration governed by this ToolProxy." .

lti:security_contract rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ToolProxy ;
  rdfs:range lti:SecurityContract ;
  rdfs:comment "The security aspect of an integration contract between two parties." .

lti:tool_consumer_profile rdf:type owl:FunctionalProperty ;
  rdfs:domain lti:ToolProxy ;
  rdfs:range lti:ToolConsumerProfile ;
  rdfs:comment "The profile of the Tool Consumer that is participating in the integration governed by this ToolProxy." .

